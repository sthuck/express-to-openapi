{
  "name": "express-to-openapi",
  "version": "0.1.0",
  "description": "Generate OpenAPI 3.0 specifications from TypeScript Express servers via static AST analysis",
  "packageManager": "yarn@4.6.0",
  "type": "module",
  "main": "./dist/index.mjs",
  "bin": "./dist/index.mjs",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "openapi",
    "swagger",
    "express",
    "typescript",
    "api",
    "documentation",
    "generator",
    "ast",
    "static-analysis"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sthuck/express-to-openapi.git"
  },
  "homepage": "https://github.com/sthuck/express-to-openapi#readme",
  "bugs": {
    "url": "https://github.com/sthuck/express-to-openapi/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "commander": "^14.0.2",
    "openapi-types": "^12.1.3",
    "ts-morph": "^27.0.2",
    "typeconv": "^2.3.1",
    "typescript": "^5.9.3",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.6",
    "@vitest/coverage-v8": "4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "express": "^5.2.1",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16",
    "zod": "^4.3.5"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.mjs",
    "dev": "tsx src/index.mts",
    "test": "vitest",
    "lint": "eslint .",
    "format": "prettier --write src/**/*.ts"
  }
}
